{
    "name": "@onlook/repo",
    "version": "0.0.0",
    "description": "Onlook Monorepo",
    "homepage": "https://onlook.com",
    "license": "Apache-2.0",
    "private": true,
    "type": "module",
    "author": {
        "name": "Onlook",
        "email": "contact@onlook.com"
    },
    "workspaces": [
        "apps/web/client",
        "packages/*",
        "tooling/*"
    ],
    "scripts": {
        "build": "bun --filter @onlook/web-client build",
        "start": "bun --filter @onlook/web-client start",
        "dev": "bun --filter @onlook/web-client dev",
        "test": "bun --filter @onlook/web-client test",
        "format": "bun --filter @onlook/web-client format",
        "lint": "bun --filter @onlook/web-client lint",
        "typecheck": "bun --filter @onlook/web-client typecheck",
        "clean": "git clean -xdf node_modules"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/onlook-dev/onlook.git"
    },
    "bugs": {
        "url": "https://github.com/onlook-dev/onlook/issues"
    },
    "devDependencies": {
        "@onlook/eslint": "*"
    },
    "packageManager": "bun@1.3.1"
}
